{
  "name": "@genapi/core",
  "type": "module",
  "version": "3.7.1",
  "author": "Hairyf <wwu710632@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/genapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/genapi.git"
  },
  "bugs": "https://github.com/hairyf/genapi/issues",
  "keywords": [
    "genapi",
    "parser"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "genapi": "./bin/index.dev.mjs"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "automd": "automd",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@genapi/shared": "workspace:*",
    "@hairy/utils": "catalog:runtime",
    "cac": "catalog:runtime",
    "chalk": "catalog:runtime",
    "fs-extra": "catalog:runtime",
    "jiti": "catalog:runtime",
    "ora": "catalog:runtime",
    "unconfig": "catalog:runtime"
  },
  "devDependencies": {
    "@types/fs-extra": "catalog:types",
    "@types/node": "catalog:types"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs"
      },
      "./package.json": "./package.json"
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "bin": {
      "genapi": "./bin/index.mjs"
    }
  }
}
