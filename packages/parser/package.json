{
  "name": "@genapi/parser",
  "type": "module",
  "version": "3.7.1",
  "author": "Hairyf <wwu710632@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/hairyf/genapi#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hairyf/genapi.git"
  },
  "bugs": "https://github.com/hairyf/genapi/issues",
  "keywords": [
    "genapi",
    "shared"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./package.json": "./package.json"
  },
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "nr build"
  },
  "dependencies": {
    "@genapi/shared": "workspace:*",
    "@genapi/transform": "workspace:*",
    "@hairy/utils": "catalog:runtime",
    "pascal-case": "catalog:runtime",
    "transliteration": "catalog:runtime"
  },
  "devDependencies": {
    "openapi-specification-types": "catalog:openapi"
  },
  "publishConfig": {
    "exports": {
      ".": "./dist/index.mjs",
      "./package.json": "./package.json"
    }
  }
}
